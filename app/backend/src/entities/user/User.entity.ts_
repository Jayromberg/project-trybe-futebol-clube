export enum Role {
  user = 'user',
  admin = 'admin',
}

export default abstract class User {
  private _email: string;
  private _password: string;
  private _role: Role;

  constructor(email: string, password: string, role: Role) {
    this._email = email;
    this._password = password;
    this._role = role;
  }
}
